{
    "Logging": {
        "Logging": {
            "Debug": {
                "LogLevel": {
                    "Default": "Information"
                }
            },
            "LogLevel": {
                "Default": "Information",
                "Microsoft.AspNetCore": "Warning"
            }
        }
    },
    "LocalizationSettings": {
        "ResourcesPath": "Resources",
        "Cultures": [ "en-US", "ru-RU" ]
    },
    "CookieAuthenticationSettings": {
        "ApiPath": [ "/cp-api" ],
        "LoginPath": "/login",
        "CookieName": ".AspNetCore.Cookies",
        "ExpireTime": 30
    },
    "AppSettings": {
        "Description": "Demo Market project.",
        "Name": "Market"
    },
    "IdentitySettings": {
        "SameUserNameEmail": true
    }
}
